{
  "name": "@adobe/helix-universal-logger",
  "version": "2.0.6",
  "description": "Helix Universal Logger",
  "main": "src/index.js",
  "scripts": {
    "test": " nyc mocha",
    "test-ci": "nyc mocha --reporter xunit --reporter-options output=./junit/test-results.xml && codecov",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "docs": "npx jsdoc2md -c .jsdoc.json --files 'src/*.js'  > docs/API.md",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-universal-logger"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-universal-logger/issues"
  },
  "homepage": "https://github.com/adobe/helix-universal-logger#readme",
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.3.2",
    "@adobe/helix-shared-wrap": "1.0.4",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "ajv": "8.11.0",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.13.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.26.0",
    "jsdoc-to-markdown": "7.1.1",
    "junit-report-builder": "3.0.0",
    "lint-staged": "12.3.7",
    "mocha": "9.2.2",
    "nock": "13.2.4",
    "nyc": "15.1.0",
    "semantic-release": "19.0.2"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "dependencies": {
    "@adobe/helix-fetch": "3.0.8",
    "@adobe/helix-log": "6.0.0"
  }
}
